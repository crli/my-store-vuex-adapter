{
    "name": "my-store-vuex-adapter",
    "version": "0.0.1",
    "description": "my-store-adapter对应vuex的适配器，可以将my-store-adapter的module适配到vuex的mutation",
    "main": "dist/index.js",
    "directories": {
      "test": "test"
    },
    "scripts": {
      "test": "cross-env BABEL_ENV=test karma start",
      "dev": "webpack --config ./build/webpack.base.conf.js --progress --hide-modules --watch",
      "build": "webpack --config ./build/webpack.prod.conf.js --progress --hide-modules",
      "doc": "docjsx c md ./doc-jsx/README.jsx ./ && docjsx c html ./doc-jsx/README.jsx ./"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/crli/my-store-vuex-adapter.git"
    },
    "keywords": [
